<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav_graph">

    <action
        android:id="@+id/action_global_startFlowFragment"
        app:destination="@id/startFlowFragment"
        app:popUpTo="@id/nav_graph" />

    <action
        android:id="@+id/action_global_mainFlowFragment"
        app:destination="@id/mainFlowFragment"
        app:popUpTo="@id/nav_graph" />

    <fragment
        android:id="@+id/mainFlowFragment"
        android:name="com.vladbystrov.findevent.main.MainFlowFragment"
        android:label="fragment_main_flow"
        tools:layout="@layout/fragment_main_flow"/>
    <fragment
        android:id="@+id/startFlowFragment"
        android:name="com.vladbystrov.findevent.start.presentation.StartFlowFragment"
        android:label="fragment_start_flow"
        tools:layout="@layout/fragment_start_flow" />
</navigation>